<templateSet group="Teo">
    <template name="model" value="model $modelName$ {&#10;  $END$&#10;}&#10;" description="Declare a model"
              toReformat="true" toShortenFQNames="true">
        <variable name="modelName" expression="" defaultValue="&quot;ModelName&quot;" alwaysStopAt="true"/>
        <context>
            <option name="TEO_TOP" value="true"/>
        </context>
    </template>
    <template name="enum" value="enum $enumName$ {&#10;  $END$&#10;}&#10;" description="Declare an enum"
              toReformat="true" toShortenFQNames="true">
        <variable name="enumName" expression="" defaultValue="&quot;EnumName&quot;" alwaysStopAt="true"/>
        <context>
            <option name="TEO_TOP" value="true"/>
        </context>
    </template>
    <template name="let" value="let $constantName$ = $expression$$END$" description="Declare a constant"
              toReformat="false" toShortenFQNames="true">
        <variable name="constantName" expression="" defaultValue="&quot;constantName&quot;" alwaysStopAt="true"/>
        <variable name="expression" expression="" defaultValue="&quot;EXPRESSION&quot;" alwaysStopAt="true"/>
        <context>
            <option name="TEO_TOP" value="true"/>
        </context>
    </template>
    <template name="import" value="import $END$" description="Import a file"
              toReformat="false" toShortenFQNames="true">
        <context>
            <option name="TEO_TOP" value="true"/>
        </context>
    </template>
    <template name="import from" value="import { $identifier$ } from $END$" description="Import from a file"
              toReformat="false" toShortenFQNames="true">
        <variable name="identifier" expression="" defaultValue="&quot;IDENTIFIER&quot;" alwaysStopAt="true"/>
        <context>
            <option name="TEO_TOP" value="true"/>
        </context>
    </template>
</templateSet>
